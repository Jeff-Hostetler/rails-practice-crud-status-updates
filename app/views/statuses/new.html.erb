<h1>New Status</h1>
<br>
  <% if @status.errors.present? %>
    <%= @status.errors.full_messages.each do |message| %>
    <%= message %>
    <% end %>
  <% end %>
<%= form_for @status do |f| %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <br>
  <%= f.label :status %>
  <%= f.text_field :status %>
  <br>
  <%= f.label :likes %>
  <%= f.number_field :likes %>
  <br>
  <%= f.submit %>
<% end %>
